{
  "multipart": [
    {
      "apply": { "model": "catwalksinc:block/catwalk_grate_floor" }
    },
    {
      "when": { "north": true },
      "apply": { "model": "catwalksinc:block/catwalk_handrail" }
    },
    {
      "when": { "east": true },
      "apply": { "model": "catwalksinc:block/catwalk_handrail", "uvlock": true, "y": 90 }
    },
    {
      "when": { "south": true },
      "apply": { "model": "catwalksinc:block/catwalk_handrail", "uvlock": true, "y": 180 }
    },
    {
      "when": { "west": true },
      "apply": { "model": "catwalksinc:block/catwalk_handrail", "uvlock": true, "y": 270 }
    }
  ]
}